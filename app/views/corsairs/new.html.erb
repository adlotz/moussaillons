<h1>Inscrit toi! </h1>

<%= form_with(model: @corsair, local: true) do |form| %>
  
  <p>
    <%= form.label :first_name %><br>
    <%= form.text_field :first_name %>
  </p>
 
  <p>
    <%= form.label :age %><br>
    <%= form.text_area :age %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
 
<% end %>